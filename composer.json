{
  "name": "casco/amqp",
  "type": "library",
  "config": {
    "sort-packages": true
  },
  "require": {
    "ext-pcntl": "^7.2",
    "php-amqplib/php-amqplib": "^2.8",
    "prolic/humus-amqp": "^1.4",
    "symfony/console": "^4.3",
    "symfony/http-kernel": "^4.3"
  },
  "require-dev": {
    "phpunit/phpunit": "^8.0",
    "treehouselabs/standards": "^0.2.0"
  },
  "autoload": {
    "psr-4": {
      "Amqp\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tests\\": "tests/"
    }
  }
}
